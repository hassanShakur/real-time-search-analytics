<%
=begin%>
 <% if @articles.present? %>
  <ul>
    <% @articles.each do |article| %>
      <li><%= link_to article.title, article %></li>
    <% end %>
  </ul>
<% else %>
  <p>No articles found</p>
<% end %> 
<%
=end%>

<% @articles.each do |article| %>
  <%= render article %>
  <p>
    <%= link_to "Show this article", article %>
  </p>
<% end %>
